generator client {
  provider = "prisma-client-js"
}

model UserCredential {
  id       String @id
  raw_id   String
  response Json
  type     String
  email    String @unique
}

model User {
  email String @unique
  id    String @id
}

model Session {
  sid     String   @id
  sess    Json
  expire  DateTime 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
